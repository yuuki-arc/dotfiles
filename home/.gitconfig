[include]
	path = ~/.gitconfig.local

[user]
	name = yuuki-arc
	email = arcs7nine@gmail.com
[core]
	excludesfile = ~/.gitignore_global
	editor = vim -f
	precomposeunicode = true
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /usr/local/Caskroom/sourcetree/1.9.6/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[color]
	ui = auto
[alias]
	co = checkout
	st = status
[pager]
	log = diff-highlight 
	show = diff-highlight 
	diff = diff-highlight 
[ghq]
	root = ~/src
[push]
	default = simple
[hub]
	host = 
